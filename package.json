{
  "name": "@balena/jellyfish-worker",
  "version": "3.0.56",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-worker.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Worker library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js > README.md",
    "postinstall": "if [ -z \"$CI\" ] && [ -d \".git\" ]; then husky install; fi"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.62",
    "@balena/jellyfish-jellyscript": "^2.1.53",
    "@balena/jellyfish-logger": "^1.0.42",
    "@balena/jellyfish-uuid": "^1.0.67",
    "bluebird": "^3.7.2",
    "errio": "^1.2.2",
    "fast-equals": "^2.0.0",
    "iso8601-duration": "^1.3.0",
    "json-e": "^4.3.0",
    "lodash": "^4.17.21",
    "skhema": "^5.3.3",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.1.1",
    "ava": "^3.15.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.4.0",
    "deplint": "^1.1.3",
    "eslint": "^7.21.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "husky": "^5.1.3",
    "jsdoc-to-markdown": "^7.0.0",
    "lint-staged": "^10.5.4",
    "fast-json-patch": "^3.0.0-1",
    "husky": "^5.0.8",
    "sinon": "^9.2.4"
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc",
      "git add README.md"
    ]
  }
}
