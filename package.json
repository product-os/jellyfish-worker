{
  "name": "@balena/jellyfish-worker",
  "version": "37.1.8",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-worker.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "description": "Worker library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json && copyup lib/contracts/**/*.md build",
    "lint": "balena-lint lib test && deplint && prettier --check **/*.json **/*.yml",
    "lint:fix": "balena-lint --fix lib test && prettier -w **/*.json **/*.yml",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest test/unit",
    "test:integration": "find test/integration -name *.spec.ts | xargs -L 1 npx jest",
    "doc": "typedoc ./lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up",
    "types": "npm run build && autumndb generate-contract-interfaces && balena-lint --fix lib/types/contracts"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "lib/contracts/*.ts": [
      "npm run types",
      "git add lib/types/contracts/*"
    ],
    "*.ts": [
      "balena-lint --fix"
    ],
    "*.{json,yml}": [
      "prettier -w"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.103",
    "@balena/jellyfish-environment": "^14.6.10",
    "@balena/jellyfish-jellyscript": "^7.1.143",
    "@balena/jellyfish-logger": "^5.1.126",
    "@balena/jellyfish-metrics": "^2.0.291",
    "@graphile/logger": "^0.2.0",
    "@types/node": "^17.0.41",
    "autumndb": "^22.2.167",
    "axios": "^0.27.2",
    "bcrypt": "^5.1.0",
    "blueimp-md5": "^2.19.0",
    "countries-and-timezones": "^3.3.0",
    "cron-parser": "^4.7.0",
    "date-fns": "^2.29.3",
    "fast-equals": "^3.0.3",
    "fast-json-patch": "^3.1.1",
    "googleapis": "^105.0.0",
    "graphile-worker": "^0.13.0",
    "is-uuid": "^1.0.2",
    "iso8601-duration": "^2.1.1",
    "json-e": "^4.4.3",
    "just-permutations": "^2.1.1",
    "lodash": "^4.17.21",
    "nock": "^13.2.9",
    "qs": "^6.10.5",
    "semver": "^7.3.8",
    "serialize-error": "8.1.0",
    "skhema": "^6.0.6",
    "slugify": "^1.6.5",
    "typed-error": "^3.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@balena/lint": "^6.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.37",
    "@types/blueimp-md5": "^2.18.0",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^29.2.3",
    "@types/lodash": "^4.14.189",
    "@types/pg": "^8.6.5",
    "@types/semver": "^7.3.13",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "copyfiles": "^2.4.1",
    "deplint": "^1.1.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "sinon": "^14.0.2",
    "ts-jest": "^29.0.3",
    "typedoc": "^0.23.21",
    "typescript": "^4.8.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-11-19T05:22:30.916Z"
  }
}
