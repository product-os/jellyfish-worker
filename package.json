{
  "name": "@balena/jellyfish-worker",
  "version": "18.6.31",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-worker.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Worker library for Jellyfish",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/"
  ],
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "npm run clean && tsc -p tsconfig.build.json",
    "lint": "balena-lint lib test && deplint",
    "lint:fix": "balena-lint --fix lib test",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "jest ./lib",
    "test:integration": "jest test/integration",
    "test:compose": "docker build -t balena/jellyfish-sut:latest . && docker-compose -f docker-compose.test.yml -f docker-compose.yml up --exit-code-from=sut",
    "doc": "typedoc ./lib/ && touch docs/.nojekyll",
    "prepack": "npm run build",
    "compose": "docker-compose up"
  },
  "deplint": {
    "files": [
      "lib/**/*.{ts}"
    ],
    "modules": [
      "lib"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "balena-lint --fix"
    ]
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "Apache-2.0",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.2.15",
    "@balena/jellyfish-core": "^14.4.2",
    "@balena/jellyfish-jellyscript": "^5.1.40",
    "@balena/jellyfish-logger": "^4.0.37",
    "@balena/jellyfish-queue": "^4.0.47",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "bluebird": "^3.7.2",
    "fast-equals": "^2.0.4",
    "fast-json-patch": "^3.1.0",
    "iso8601-duration": "^1.3.0",
    "json-e": "^4.4.3",
    "just-permutations": "^2.0.1",
    "lodash": "^4.17.21",
    "nock": "^13.2.4",
    "qs": "^6.10.3",
    "semver": "^7.3.5",
    "serialize-error": "^8.1.0",
    "skhema": "^6.0.3",
    "typed-error": "^3.2.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@balena/jellyfish-config": "^2.0.2",
    "@balena/jellyfish-environment": "^7.0.0",
    "@balena/jellyfish-types": "^2.0.3",
    "@balena/lint": "^6.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bluebird": "^3.5.36",
    "@types/is-uuid": "^1.0.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/semver": "^7.3.9",
    "@types/sinon": "^10.0.11",
    "@types/uuid": "^8.3.4",
    "deplint": "^1.1.3",
    "jest": "^27.5.1",
    "jest-esm-transformer": "^1.0.0",
    "lint-staged": "^12.3.4",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.7.0",
    "sinon": "^13.0.1",
    "ts-jest": "^27.1.3",
    "typedoc": "^0.22.12",
    "typescript": "^4.5.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "versionist": {
    "publishedAt": "2022-02-19T02:58:18.482Z"
  }
}
