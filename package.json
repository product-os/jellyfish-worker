{
  "name": "@balena/jellyfish-worker",
  "version": "1.0.199",
  "repository": {
    "type": "git",
    "url": "https://github.com/product-os/jellyfish-worker.git"
  },
  "engines": {
    "node": ">=14.2.0"
  },
  "description": "Worker library for Jellyfish",
  "main": "lib/index.js",
  "directories": {
    "lib": "lib"
  },
  "deplint": {
    "files": [
      "lib/**/*.{js}"
    ],
    "modules": [
      "lib"
    ]
  },
  "scripts": {
    "test": "npm run doc && catch-uncommitted --skip-node-versionbot-changes && make lint && make test",
    "doc": "jsdoc2md --template doc/README.hbs lib/index.js > README.md"
  },
  "author": "Balena.io. <hello@balena.io>",
  "license": "UNLICENSED",
  "dependencies": {
    "@balena/jellyfish-assert": "^1.0.29",
    "@balena/jellyfish-jellyscript": "^1.0.42",
    "@balena/jellyfish-logger": "^0.0.147",
    "@balena/jellyfish-uuid": "^1.0.31",
    "bluebird": "^3.7.2",
    "errio": "^1.2.2",
    "fast-equals": "^2.0.0",
    "iso8601-duration": "^1.2.0",
    "json-e": "^4.3.0",
    "lodash": "^4.17.20",
    "skhema": "^5.3.3",
    "typed-errors": "^1.1.0"
  },
  "devDependencies": {
    "@balena/jellycheck": "^0.0.8",
    "ava": "^3.13.0",
    "catch-uncommitted": "^1.6.2",
    "depcheck": "^1.3.1",
    "deplint": "^1.1.2",
    "eslint": "^7.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^30.7.8",
    "eslint-plugin-lodash": "^7.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "husky": "^4.3.0",
    "jsdoc-to-markdown": "^6.0.1",
    "lint-staged": "^10.5.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix",
      "npm run doc"
    ]
  }
