{
  "extends": [
    "github>product-os/renovate-config"
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "devDependencies",
        "peerDependencies"
      ],
      "matchPackagePatterns": ["@balena/jellyfish-*"],
      "matchUpdateTypes": ["patch"],
      "enabled": false
    },
    {
      "matchPackageNames": ["resinci/jellyfish-test"],
      "groupName": "jellyfish-test-image"
    },
    {
      "matchPackageNames": ["serialize-error"],
      "enabled": false
    }
  ]
}
